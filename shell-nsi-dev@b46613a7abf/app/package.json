{
  "name": "orchestrator",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node app-config/createConfig.js ./src && ng serve --project shell --open",
    "start:proxy": "node app-config/createConfig.js ./src && ng serve --project shell --proxy-config proxy.config.json",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "build": "node app-config/createConfig.js ./src && ng build",
    "build:dev": "node app-config/createConfig.js ./src && ng build --configuration=staging",
    "build:master": "node app-config/createConfig.js ./src && ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start:shell": "ng serve --project shell --open",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint-staged": "lint-staged -r"
  },
  "private": true,
  "dependencies": {
