{
  "groupList": [
    {
      "view": "cardgroup",
      "name": "base-info",
      "label": "Основные сведения",
      "attrList": [
        "parentCommissionOrg",
        "start",
        "end",
        "number",
        "plotsType"
      ]
    },
    {
      "view": "cardgroup",
      "name": "slicingDoc",
      "label": "Документ о нумерации",
      "attrList": [
        "numerRes",
        "numerResNum",
        "numerResDate"
      ]
    }
  ],
  "attrList": [
    {
      "name": "parentCommissionOrg",
      "type": "string",
      "view": "async-select",
      "label": "Вышестоящая комиссия",
      "props": {
        "url": "https://gas.apps.dev.devsun.ru/service/registry/commission",
        "filter": "subjectRF.SUBJCOD==68 and commissionType.VNKOD==4 and status==ACTIVE",
        "schema": "CommissionComposition",
        "idField": "commissionOrg.id",
        "labelField": "commissionOrg.commissionName",
        "outgoingLink": true
      },
      "mandatory": true
    },
    {
      "name": "start",
      "view": "int",
      "type": "int",
      "label": "Начальный номер участка",
      "mandatory": true
    },
    {
      "name": "end",
      "type": "int",
      "view": "int",
      "label": "Конечный номер участка",
      "mandatory": true
    },
    {
      "name": "number",
      "view": "int",
      "type": "int",
      "label": "Количество участков",
      "mandatory": true
    },
    {
      "name": "plotsType",
      "type": "string",
      "view": "select",
      "label": "Тип избирательного участка",
      "mandatory": true,
      "createReadOnly": true,
      "enum": [
        {
          "value": "Постоянный",
          "key": "permanent"
        },
        {
          "value": "Временный",
          "key": "temporary"
        }
      ]
    },
    {
      "name": "numerRes",
      "view": "async-select",
      "type": "string",
      "label": "Наименование документа",
      "props": {
        "schema": "NSI945",
        "idField": "VNKOD",
        "labelField": "ZNACHATR",
        "outgoingLink": true
      },
      "mandatory": true
    },
    {
      "name": "numerResNum",
      "view": "int",
      "type": "int",
      "label": "Номер документа",
      "mandatory": true
    },
    {
      "name": "numerResDate",
      "view": "date",
      "type": "date",
      "label": "Дата",
      "mandatory": true
    }
  ],
  "grid": {
    "start": {
      "col": 3
    },
    "end": {
      "col": 3
    },
    "number": {
      "col": 3
    },
    "plotsType": {
      "col": 3
    },
    "numerRes": {
      "col": 4
    },
    "numerResNum": {
      "col": 4
    },
    "numerResDate": {
      "col": 4
    }
  }
}
