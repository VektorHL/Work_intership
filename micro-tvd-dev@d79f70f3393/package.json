{
  "name": "tvd",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "node app-config/createConfig.js ./src && ng serve --project tvd --open",
    "start:mock": "node app-config/createConfig.js ./src && yarn --cwd ./__mocks/ watch",
    "start:proxy": "node app-config/createConfig.js ./src && ng serve --project tvd --proxy-config proxy.config.json",
    "build": "node app-config/createConfig.js ./src && ng build",
    "build:dev": "node app-config/createConfig.js ./src && ng build --configuration=staging --deploy-url='/mfe/tvd/'",
    "build:master": "node app-config/createConfig.js ./src && ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "commit": "commit-wizard",
    "lint-staged": "lint-staged -r",
    "graphql-json-to-sdl": "graphql-json-to-sdl src/app/schema.json src/app/schema.graphql"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^12.1.1",
    "@angular/animations": "~12.1.1",
    "@angular/cdk": "^12.1.1",
    "@angular/common": "~12.1.1",
    "@angular/compiler": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@apollo/client": "^3.3.21",
    "@cikrf/gas-components": "^1.0.6",
    "@cikrf/gas-ui-kit": "^2.1.188",
    "@cikrf/gas-utils": "^2.0.18",
    "@devsun/utility-event-bus": "^1.0.0-SNAPSHOT.1",
    "@ngneat/until-destroy": "^8.1.1",
    "@ngrx/store": "^12.3.0",
    "@ngrx/store-devtools": "^12.3.0",
    "apollo-angular": "^2.6.0",
    "date-fns": "^2.22.1",
    "graphql": "^15.5.1",
    "js-base64": "^3.7.0",
    "lodash": "^4.17.21",
    "ng-dynamic-component": "^8.0.1",
    "ng-module-federation": "^12.1.0",
    "resize-observer": "^1.0.2",
    "rxjs": "~6.6.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.5",
    "@angular-eslint/builder": "12.2.0",
    "@angular-eslint/eslint-plugin": "12.2.0",
    "@angular-eslint/eslint-plugin-template": "12.2.0",
    "@angular-eslint/schematics": "^12.3.1",
    "@angular-eslint/template-parser": "12.2.0",
    "@angular/cli": "~12.2.5",
    "@angular/compiler-cli": "~12.1.1",
    "@cikrf/eslint-config-gas": "1.1.3",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.18.1",
    "@types/faker": "^5.5.7",
    "@types/jasmine": "~3.6.0",
    "@types/lodash": "^4.14.172",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "^4.28.4",
    "codelyzer": "^6.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "graphql-json-to-sdl": "^0.2.3",
    "jasmine-core": "~3.7.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "pre-git": "^3.17.1",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.2"
  },
  "resolutions": {
    "webpack": "^5.4.0"
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "config": {
    "pre-git": {
      "enabled": true,
      "msg-pattern": "^#([\\w]{1,5}-)?\\d+ (fix|add|del|mod): ([^>]+?)(> [^>]+?)?$",
      "commit-msg": "",
      "pre-push": [],
      "pre-commit": [
        "yarn lint-staged"
      ],
      "post-commit": [
        "git update-index --again"
      ],
      "post-checkout": [],
      "post-merge": [],
      "allow-untracked-files": true
    }
  }
}
